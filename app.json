{
  "expo": {
    "name": "zelenko",
    "slug": "zelenko",
    "scheme": "zelenko",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.maradil.zelenko"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Dozvolite aplikaciji Zelenko pristup va≈°im fotografijama.",
          "cameraPermission": "Dozvolite aplikaciji Zelenko pristup kameri."
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#9CF089",
          "image": "./assets/splash-image.png",
          "dark": {
            "image": "./assets/splash-image.png",
            "backgroundColor": "#56A082"
          },
          "imageWidth": 150
        }
      ]
    ]
  }
}
